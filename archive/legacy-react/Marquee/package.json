{
  "name": "scientific-peptide-marquee",
  "version": "1.0.0",
  "description": "Interactive React marquee component showcasing verified scientific quotes about therapeutic peptides",
  "author": "Research Peptide Company",
  "license": "MIT",
  "keywords": ["react", "marquee", "peptides", "scientific", "quotes", "interactive", "typescript"],
  "main": "index.html",
  "homepage": ".",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "vite",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "serve": "npm run build && npm run preview"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "dotenv": "^17.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
